<% flash.each do |success,msg| %>
  <div class="alert alert-primary mb-0" role="alert">
    <%= msg %>
  </div>
<% end %>
<header class="hero"> 
  <nav class="navbar max-width-auto">
    <i class="fa-sharp fa-solid fa-bars" id="hamburgerMenu"></i>
    <i class="fa-sharp fa-solid fa-times close-menu" style="display: none" id="closeHamBurger"></i>
    <div class="drawer">
      <div class="mobileNav">
        <ul>
          <li><li><a  data-bs-toggle="modal" data-bs-target="#exampleModal" data-bs-whatever="@mdo">About Us</a></li>
          <li><li><a  data-bs-toggle="modal" data-bs-target="#exampleModal" data-bs-whatever="@mdo" style='margin-bottom: 11px'>Job Search</a></li>
          <li>
            <a data-bs-toggle="modal" data-bs-target="#exampleModal" data-bs-whatever="@mdo">Employer Post / Job</a>
          </li>
        </ul>
      </div>
    </div>
    <div class="n-left">
      <%= image_tag  "https://www.jobget.com/landing-pages-2021-multimedia-assets/LOGO.svg",:class => "logo"%>
      <a class="job" data-bs-toggle="modal" data-bs-target="#exampleModal" data-bs-whatever="@mdo">About Us</a>
    </div>
    <div class="n-right">
      <a class="job" data-bs-toggle="modal" data-bs-target="#exampleModal" data-bs-whatever="@mdo"> Job Search</a>
      <a class="job" data-bs-toggle="modal" data-bs-target="#exampleModal" id="post" data-bs-whatever="@mdo"> Employer Post / Job</a>
    </div>
  </nav>
  <div class="hero-content">
    <div class="textContent">
      <span>
        Apply Today, <br />
        Start Tomorrow
      </span>
      <span> Talk to top employers in minutes. </span>
      <a data-bs-toggle="modal" data-bs-target="#exampleModal" data-bs-whatever="@mdo">Get Hired Today</a>
      <div class="nativeAppLinks">
        <div class="apple">
            <%=image_tag "apple.png"%>
          <div class="rating">
            <span>&#9733;</span>
            <span>&#9733;</span>
            <span>&#9733;</span>
            <span>&#9733;</span>
            <span>&#9733;</span>
            <span>4.9/5.0</span>
          </div>
        </div>
        <div class="google">
          <%= image_tag  "google.png" %>
          <div class="rating">
            <span>&#9733;</span>
            <span>&#9733;</span>
            <span>&#9733;</span>
            <span>&#9733;</span>
            <span>&#9733;</span>
            <span>4.7/5.0</span>
          </div>
        </div>
      </div>
    </div>
    <%= image_tag  "jobSeeker.jpg",:class => "jobSeekers fadein"%>
  </div>
</header>
<main class="maincontent ">
  <section class="featuredInRow max-width-auto">
    <span class="asSeen">As Seen On</span>
    <%= image_tag  "tc-currunch.png" %>
